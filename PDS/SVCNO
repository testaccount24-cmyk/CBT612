   svc.255='ISC'
   IF svcno = '' THEN svcno = 'ALL'
   IF RIGHT(svcno,1) = x THEN svcno = X2D(LEFT(svcno,LENGTH(svcno)-1))
   IF svcno = 'ALL' THEN
      DO  i = 1 to 139
          SAY 'SVC ' i '('D2X(i)') is ' svc.i;
      END
   ELSE SAY 'SVC ' svcno 'is ' svc.svcno
 EXIT
